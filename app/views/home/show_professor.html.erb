<p id="notice"><%= notice %></p>

    <div class="container">


        <h2 class="title">Lista de profesores</h2>

      <div class="cards">

      <% @professors.each do |professor| %>



        <div class="card">
          <div class="card__inner">
            <i class="fa fa-user-tie"></i>
            <p><%= professor.name %> <%= professor.father_last_name %> <%= professor.mother_last_name %></p>
            <%= link_to professor_path(professor) do %>
              <i class="btn fas fa-info-circle fa-sm" aria-hidden="true"></i>
            <%end%>
          </div>
        </div>

      <%end%>
      </div>
    </div>
    <script src='http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>
