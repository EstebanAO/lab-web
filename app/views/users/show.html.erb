<div class="container">
  <h2 class="title">Mostrar Usuario</h2>
    <p class="text-center">
      <strong>Correo:</strong>
      <%= @user.email %>
    </p>

    <p class="text-center">
      <strong>Administrador:</strong>
      <%= @user.admin %>
    </p>

  <p class="text-center">
    <%= link_to 'Editar',  edit_user_path(@user), :class => 'btn btn-sm btn-primary'%> |
    <%= link_to 'Regresar', users_path, :class => 'btn btn-sm btn-primary' %>
  </p>
</div>



